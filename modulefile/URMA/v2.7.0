#setenv WGRIB2LIB /meso/save/Ying.Lin/pcpanl/urma.v2.7.0/lib/grib2/lib/libwgrib2.a
#setenv WGRIB2INC /meso/save/Ying.Lin/pcpanl/urma.v2.7.0/lib/grib2/lib
